= About;\nexport default About;\nvar _c;\n$RefreshReg$(_c, \"About\");","map":{"version":3,"names":["React","useNavigate","Link","jsxDEV","_jsxDEV","About","_s","navigate","handleContactClick","className","children","fileName","_jsxFileName","lineNumber","columnNumber","to","onClick","_c","$RefreshReg$"],"sources":["/Users/andriimakukha/Desktop/Hillel/lesson-44/src/components/About.jsx"],"sourcesContent":["import React from 'react';\nimport { useNavigate, Link } from 'react-router-dom';\n\nconst About = () => {\n    const navigate = useNavigate();\n\n    const handleContactClick = () => {\n        navigate('/contact');\n    };\n\n    return (\n        <div className=\"about-container\">\n            <h1>Про нас</h1>\n            <p>\n                Ми — команда ентузіастів автомобільного світу, яка прагне надати вам найкращий досвід у виборі\n                автомобіля. Наша місія полягає в тому, щоб допомогти кожному знайти ідеальне транспортне засіб\n                відповідно до його потреб і бюджету.\n            </p>\n            <p>\n                Наш автосалон пропонує широкий асортимент нових та вживаних автомобілів, які проходять\n                ретельну перевірку якості. Ми співпрацюємо лише з перевіреними постачальниками та виробниками,\n                щоб забезпечити наших клієнтів найкращими продуктами на ринку.\n            </p>\n            <h2>Наша команда</h2>\n            <p>\n                Наша команда складається з досвідчених фахівців, які готові відповісти на всі ваші запитання,\n                надати професійну консультацію та допомогти у виборі. Ми цінуємо кожного клієнта і прагнемо\n                забезпечити високий рівень обслуговування на всіх етапах покупки.\n            </p>\n            <h2>Чому обирають нас?</h2>\n            <ul>\n                <li>Широкий асортимент автомобілів.</li>\n                <li>Конкурентні ціни та вигідні пропозиції.</li>\n                <li>Індивідуальний підхід до кожного клієнта.</li>\n                <li>Гарантія якості та безпеки автомобілів.</li>\n            </ul>\n            <p>\n                Дякуємо, що обрали нас! Ми сподіваємося, що ви знайдете свій ідеальний автомобіль у нашому\n                автосалоні. Якщо у вас є запитання, будь ласка, <Link to=\"/contact\">зв'яжіться з нами</Link>.\n            </p>\n            <button className=\"about-button\" aria-label=\"Зв'язатися з нами\" onClick={handleContactClick}>\n                Зв'язатися з нами\n            </button>\n        </div>\n    );\n};\n\nexport default About;\n"],"mappings":";;AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,WAAW,EAAEC,IAAI,QAAQ,kBAAkB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAErD,MAAMC,KAAK,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAChB,MAAMC,QAAQ,GAAGN,WAAW,CAAC,CAAC;EAE9B,MAAMO,kBAAkB,GAAGA,CAAA,KAAM;IAC7BD,QAAQ,CAAC,UAAU,CAAC;EACxB,CAAC;EAED,oBACIH,OAAA;IAAKK,SAAS,EAAC,iBAAiB;IAAAC,QAAA,gBAC5BN,OAAA;MAAAM,QAAA,EAAI;IAAO;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAI,CAAC,eAChBV,OAAA;MAAAM,QAAA,EAAG;IAIH;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAG,CAAC,eACJV,OAAA;MAAAM,QAAA,EAAG;IAIH;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAG,CAAC,eACJV,OAAA;MAAAM,QAAA,EAAI;IAAY;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAI,CAAC,eACrBV,OAAA;MAAAM,QAAA,EAAG;IAIH;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAG,CAAC,eACJV,OAAA;MAAAM,QAAA,EAAI;IAAkB;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAI,CAAC,eAC3BV,OAAA;MAAAM,QAAA,gBACIN,OAAA;QAAAM,QAAA,EAAI;MAA+B;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eACxCV,OAAA;QAAAM,QAAA,EAAI;MAAuC;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAChDV,OAAA;QAAAM,QAAA,EAAI;MAAyC;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAClDV,OAAA;QAAAM,QAAA,EAAI;MAAuC;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC;IAAA;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAChD,CAAC,eACLV,OAAA;MAAAM,QAAA,GAAG,wrBAEiD,eAAAN,OAAA,CAACF,IAAI;QAACa,EAAE,EAAC,UAAU;QAAAL,QAAA,EAAC;MAAiB;QAAAC,QAAA,EAAA